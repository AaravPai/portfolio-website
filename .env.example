# Environment Variables Template
# Copy this file to .env.local and fill in your values

# EmailJS Configuration
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Analytics (Optional)
VITE_GA_TRACKING_ID=your_google_analytics_id_here

# Site Configuration
VITE_SITE_URL=https://yourportfolio.com
VITE_SITE_NAME=Your Portfolio
VITE_SITE_DESCRIPTION=Professional portfolio showcasing projects and skills

# Contact Information
VITE_CONTACT_EMAIL=your.email@example.com
VITE_GITHUB_URL=https://github.com/yourusername
VITE_LINKEDIN_URL=https://linkedin.com/in/yourusername
VITE_TWITTER_URL=https://twitter.com/yourusername

# Development
VITE_DEV_MODE=false